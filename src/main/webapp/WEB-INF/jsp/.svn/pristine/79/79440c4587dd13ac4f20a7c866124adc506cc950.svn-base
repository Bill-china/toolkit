    function UnionArea(options){
        this.options = options;
        if(typeof(this.options.province) === "string"){
            this.options.province = document.getElementById(this.options.province);
        }
        if(typeof(this.options.city) === "string"){
            this.options.city = document.getElementById(this.options.city);
        }
        if(!this.options.areaArr){
            this.options.areaArr = addr_data;
        }
        this.init();
    }
    UnionArea.prototype = {
        init:function(){
            var options = this.options, __self = this;
            this.clear(options.province);
            this.clear(options.city);
            options.province.add(new Option("请选择省份","0"));
            options.city.add(new Option("请选择城市","0"));
            for(var i = 0, l = options.areaArr.length; i < l; i++){
                if(options.areaArr[i].areacode == 0){
                    continue;
                }
                if(options.areaArr[i].parentareacode == 0){
                    option = new Option(options.areaArr[i].areaname, options.areaArr[i].areacode);
                    options.province.add(option);
                }
            }
            options.province.onchange = function(){
                var __parent = this.value;
                __self.clear(options.city);
                if(__parent == 0){
                    options.city.add(new Option("请选择城市",0));
                }else{
                    for(var i = 0, l = options.areaArr.length; i < l; i++){
                        if(options.areaArr[i].parentareacode == __parent || options.areaArr[i].areacode == __parent){
                            option = new Option(options.areaArr[i].areaname,options.areaArr[i].areaname);
                            options.city.add(option);
                        }
                    }
                }
            }
        },
        set:function(province, city){
            var options = this.options;
            if(province && city){
                options.province.selectedIndex = province;
                options.province.onchange();
                for(var i = 0, l = options.city.length; i < l; i++){
                    if(options.city.options[i].value == city){
                        options.city.selectedIndex = i;
                        break;
                    }
                }
            }
        },
        clear:function(obj){
            obj.options.length = 0;
        }
    }
    window.UnionArea = UnionArea;

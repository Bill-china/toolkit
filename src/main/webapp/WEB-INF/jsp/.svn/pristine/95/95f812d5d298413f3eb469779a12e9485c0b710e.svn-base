<script>
var d=(new Date).getTime(),e=null,f=null,g=!1,h=function(b,a){var c=window;c.addEventListener?c.addEventListener(b,a,!1):c.attachEvent?c.attachEvent("on"+b,a):c["on"+b]=a},i=function(){if(!g&&null!=e&&null!=f){g=!0;var b=["//www.google.com/csi?s=csa&v=3&action=qihoojs&rt=ol.",e,",palt.",f].join(""),a=new Image;a.onload=a.onerror=function(){a.onload=a.onerror=null};a.src=b}};window.googTimeAdsLoaded=function(){f=(new Date).getTime()-d};h("load",function(){e=(new Date).getTime()-d});h("unload",function(){i()});
